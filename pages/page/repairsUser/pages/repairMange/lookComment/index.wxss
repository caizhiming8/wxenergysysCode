/* pages/page/repairsUser/pages/repairMange/lookComment/index.wxss */
page {
  background-color: #f1f1f1;
}
.detail {
  top: 0;
  left: 0;
  right: 0;
  bottom: 0rpx;
  overflow: auto;
}
.detail-t {
  padding: 12rpx 30rpx;
  background-color: #fff;
}
.detail-t2 {
  margin-top: 10rpx;
}
.detail-t>.t-top {
  display: flex;
  color: #282828;
  font-size: 24rpx;
  padding: 14rpx 0;
  border-bottom: 2rpx solid #EFEFEF;
}
.detail-t>.t-top>image {
  width: 34rpx;
  height: 34rpx;
  margin-right: 20rpx;
}
.t-cont {
  color: #656565;
  font-size: 28rpx;
}
.t-cont>view {
  margin-top: 30rpx;
}


.stars{
  background:url('data:image/png;base64,');
    width: 35rpx;
    height: 35rpx;
    background-size: 35rpx 35rpx;
    margin-right: 30rpx;
    transition: 600ms;
}
.on{
  background:url('data:image/png;base64,');
    width: 35rpx;
    height: 35rpx;
    background-size: 35rpx 35rpx;
    transition: 1s;
    transform: scale(1,1)
}

.pingjia-state {
  color: #989898;
  font-size: 22rpx;
}

.t-cont>.cont {
  color: #656565;
  font-size: 28rpx;
}
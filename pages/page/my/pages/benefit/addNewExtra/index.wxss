/* pages/page/my/pages/benefit/addNewExtra/index.wxss */
page {
  background-color: #f1f1f1;
}
.page-person>.item {
  display: flex;
  height: 88rpx;
  color: #656565;
  font-size: 30rpx;
  padding: 0 30rpx;
  align-items: center;
  background-color: #fff;
  border-bottom: 2rpx solid #f1f1f1;
}
.item>.title {
  width: 150rpx;
}
.item>.title2 {
  width: 210rpx;
}
.item>.cont {
  flex: 1;
  text-align: right;
}
.item>.cont>.put {
  width: 100%;
  text-align: right;
}
.place-style {
  text-align: right;
}
.item>.right {
  flex: 1;
  display: flex;
  /* width: 150rpx; */
  align-items: center;
  justify-content: flex-end;
}
.item>.right .icon {
  width: 28rpx;
  height: 25rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}
.item2>.cont {
  flex: 2;
  width: auto;
  text-align: right;
}
.item2>.right {
  flex: 1;
  width: auto;
  justify-content: center;
}
.item>.right2 .icon2 {
  width: 50rpx;
  height: 50rpx;
}
.item-btn {
  margin: auto;
  color: #fff;
  width: 690rpx;
  height: 88rpx;
  display: flex;
  font-size: 36rpx;
  margin-top: 210rpx;
  align-items: center;
  justify-content: center;
  background-color: #3AA0FB;
}
.item2 {
  height: auto;
  /* display: flex; */
  color: #656565;
  font-size: 30rpx;
  margin-top: 10rpx;
  align-items: center;
  padding: 20rpx 30rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #f1f1f1;
}
.item-input {
  width: 100%;
  min-height: 160rpx;
}
.item2-img {
  display: flex;
  color: #989898;
  font-size: 18rpx;
  margin-top: 20rpx;
  align-items: flex-end;
  justify-content: space-between;
} 
.item2-img>.img {
  position: relative;
}
.item2-img>.img>.im {
  width: 190rpx;
  height: 190rpx;
}
.item2-img>.img>.im3 {
  width: 94rpx;
  height: 94rpx;
}
.item2-img>.img>.im2 {
  top: 10rpx;
  width: 30rpx;
  right: 10rpx;
  height: 30rpx;
  position: absolute;
}

/* 
 * 图片列表
*/
.img-list {
  display: flex;
  flex-wrap: wrap;
  padding: 10rpx 25rpx;
  background-color: #fff;
}
.img-list>.list {
  width: 174rpx;
  display: flex;
  height: 174rpx;
  margin-top: 10rpx;
  position: relative;
  align-items: center;
  justify-content: center;
}
.img-list>.list>.icon {
  width: 166rpx;
  height: 166rpx;
}
.img-list>.list>.icon2 {
  width: 160rpx;
  height: 160rpx;
}
.img-list>.list>.im2 {
  top: 10rpx;
  width: 30rpx;
  right: 10rpx;
  height: 30rpx;
  position: absolute;
}

/* 底部选择框 */
.bot-choose {
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  position: absolute;
  align-items: flex-end;
  background-color: rgba(0,0,0, 0.5);
}
.bg-white {
  width: 100%;
  background-color: #ffffff;
}
.bg-white>.item {
  display: flex;
  padding: 20rpx 0;
  align-items: center;
  justify-content: center;
  border-bottom: 1rpx solid #eeeeee;
}
.bg-white>.item:last-child {
  padding: 30rpx;
  border-bottom: none;
  border-top: 10rpx solid #eeeeee;
}
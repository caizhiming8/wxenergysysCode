/* pages/page/electricity/pages/schoolEle/schoolEle.wxss */
page {
  background-color: #F1F1F1;
}
.shool-ele {
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  overflow: auto;
  position: absolute;
  flex-direction: column;
}
.shool-ele .cont-top {
  margin-bottom: 10rpx;
  background-color: #ffffff;
}
.shool-ele .cont-top>.item {
  display: flex;
  height: 130rpx;
  padding: 0 30rpx;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #f1f1f1;
}
.shool-ele .cont-top>.item>.left {
  display: flex;
  color: #656565;
  font-size: 30rpx;
  align-items: center;
}
.shool-ele .cont-top>.item>.left :last-child {
  padding-left: 30rpx;
}
.shool-ele .cont-top>.item>.left>.icon {
  width: 64rpx;
  height: 64rpx;
}
.shool-ele .cont-top>.item>.right {
  color: #3AA0FB;
  font-size: 36rx;
}
.shool-ele .cont-top>.item>.right>.led-font {
  font-size: 54rpx;
  font-family: 'UniDreamLED';
}
.shool-ele .cont-top>.item:nth-child(2) .right {
  color: #FB873A;
}
.shool-ele .cont-top>.item:nth-child(3) .right {
  color: #EB4242;
}
.shool-ele .cont-echart {
  flex: 1;
  display: flex;
  flex-direction: column;
  background-color: #ffffff;
}
.shool-ele .cont-echart>.tit {
  padding: 30rpx;
  color: #656565;
  font-size: 30rpx;
}
.shool-ele .cont-echart>.echarts {
  flex: 1;
}